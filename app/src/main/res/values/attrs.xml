<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PlayerControls">
        <attr name="changeTrackAnimDuration" format="integer"/>
        <attr name="minimumDurationForSeek" format="integer"/>
        <attr name="smallestButtonWidth" format="dimension"/>
        <attr name="currentDuration" format="integer"/>
        <attr name="totalDuration" format="integer"/>
        <attr name="state" format="enum">
            <enum name="play" value="0"/>
            <enum name="pause" value="1"/>
            <enum name="buffer" value="2"/>
        </attr>
        <attr name="iconColor" format="color"/>
        <attr name="outLineColor" format="color"/>
        <attr name="readBarsColor" format="color"/>
        <attr name="comingBarsColor" format="color"/>
        <attr name="disabledColor" format="color"/>
        <attr name="progressBackgroundColor" format="color"/>
        <attr name="previousBackgroundColor" format="color"/>
        <attr name="nextBackgroundColor" format="color"/>
    </declare-styleable>
</resources>